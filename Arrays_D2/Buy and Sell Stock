//Optimal
//TC = O(N)
//SC = O(1)

class Solution {
    public int maxProfit(int[] prices) {
        int min = Integer.MAX_VALUE;
        int pro = 0;
        for(int i : prices){
            if(i < min){
                min = i;
            }
            
            pro = Math.max(pro, (i-min));
        }
        
        return pro;
    }
}